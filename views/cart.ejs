	<section id="cart_items">
		<div class="container">
			<div class="breadcrumbs">
				<ol class="breadcrumb">
				  <li><a href="#">Home</a></li>
				  <li class="active">Shopping Cart</li>
				</ol>
			</div>
			<div class="table-responsive cart_info">
			<% if (typeof message == 'undefined') { } else {%>
			<p style="color:red; margin-left:20em;"><%=message%></p>
								<% }%>
			
				<table class="table table-condensed">
					<thead>
						<tr class="cart_menu">
							<td class="image">Item</td>
							<td class="description"></td>
							<td class="price">Price</td>
							<td class="quantity">Quantity</td>
							<td class="total">Total</td>
							<td></td>
						</tr>
					</thead>
					
					<tbody>
					<%
					for(var row in current_cart){
						var image="";var product="";var prod_id="";var price="";var quantity="";var total="";
						var column = current_cart[row];
						for(var col in column)
						{
						
						if(col=="prod_id"){prod_id=column[col];}
						if(col=="product"){product=column[col];}
						if(col=="image"){image=column[col];}
						if(col=="price"){price=column[col];}
						if(col=="quantity"){quantity=column[col];}
						}
					%>
						<tr>
							<td class="cart_product">
								<a href=""><img src="../images/<%=image%>" width="60" height="60" alt=""></a>
							</td>
							<td class="cart_description">
								<h4><a href=""><%=product%></a></h4>
								<p>Web ID: <%=prod_id%></p>
							</td>
							<td class="cart_price">
								<p><%=price%></p>
							</td>
							<td class="cart_price">
								<p><%=quantity%></p>
							</td>

							<td class="cart_total">
								<p class="cart_total_price"><%=quantity*price%></p>
							</td>
							<td class="cart_delete">
								<a class="cart_quantity_delete" href="/remove_from_cart/<%=prod_id%>"><i class="fa fa-times"></i></a>
							</td>
						</tr>
					<%
					}
					%>
						
					</tbody>
				</table>
			</div>
		</div>
	</section> <!--/#cart_items-->

