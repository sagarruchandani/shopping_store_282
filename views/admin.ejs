	        	    			    var sag= items[x];
	        	        			var keys = Object.keys(items[x]);
								   
									var value=null;
	        	       					for (var y in sag) {
	        	        					var attribute = sag[y];
	        	        					for (var z in attribute) {
	        	        					value = value+","+attribute[z];
	        	        					
	        	        											}
	        	        					
	        	        									}
									
	        	       				value= value.split(",");
	        	       				for(var i = value.length - 1; i >= 0; i--) {
 								   			if(value[i] === "undefined") {
       										value.splice(i, 1);
   										 							}
   										 	if(value[i] === "null") {
       										value.splice(i, 1);
   										 							}
   										 	if(value[i] === "") {
       										value.splice(i, 1);
   										 							}
																				}
	        	       				
	        	       				var tempo = keys[x];
									
									var i=1;
	        	        			var link_to_product="";
	        	        			%><tr><%
	        	        			for (var x in keys)
	   				     	        {
									if(x==3 || x==5) {
	   				     	        %>
	        	        			<td class="tg-031e"><%=value[x]%></td>
	        	        			<% }	
	        	        			if(x==0){  link_to_product=value[x];}
	        	        			}
	   				     	        %><td class="tg-031e"><a href="/remove_product/<%=link_to_product%>" id=<%=attribute[z]%> >Delete</a></td></tr>
	   				     	        <%
	   				     	         value = null;
	        	        			}
								}
